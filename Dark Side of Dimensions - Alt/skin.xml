<?xml version="1.0"?>
<!-- Config file for the GUI Skin (CImageGUISkin)                    -->
<!-- Source border is the border width (in pixels) on the texture.   -->
<!-- Dest border is the border width on the screen (also in pixels). -->
<!-- Order of elements dosn't matter -->
<!-- <NodeNames> are not case sensitive -->
<!-- attribute= ARE case sensitive -->
<!-- Texture   = filename              -->
<!-- SrcBorder = top,left,bottom,right -->
<!-- DstBorder = top,left,bottom,right -->
<Skin>
	<Properties>
		<Name data="The Dark Side of Darkness" />
		<Author data="Pastisha/Kadmel" />
		<Version data="1.0" />
		<Date data="2020"/>
		<Desc data="TDSOD Skin for EDOPRO"/>
		<Fallback skin="EGST_WINDOWS_CLASSIC" />
		<Font texture="fontTahoma.png" />
	</Properties>
  
	<Global guialpha="255">
		<EGDC_3D_DARK_SHADOW a="255" r="0" g="0" b="0" />		-->     	<!-- NO SE. Dark shadow for three-dimensional display elements.  -->
		<EGDC_3D_SHADOW a="255" r="255" g="255" b="0" />		-->     	<!-- Color de Lineas. Shadow color for three-dimensional display elements (for edges facing away from the light source).   -->
		<EGDC_3D_FACE a="255" r="255" g="255" b="255" />			  	<!-- NO CAMBIAR Face color for three-dimensional display elements and for dialog box backgrounds.   -->
		<EGDC_3D_HIGH_LIGHT a="255" r="0" g="0" b="0" />		-->		<!-- Color de fondo de las cajas que estan inactivas. Highlight color for three-dimensional display elements (for edges facing the light source.).   -->
		<EGDC_3D_LIGHT a="255" r="255" g="255" b="0" />		-->		        <!-- NO SE. Light color for three-dimensional display elements (for edges facing the light source.). --> 
		<EGDC_ACTIVE_BORDER a="255" r="255" g="255" b="0" />              -->		<!-- NO SE. Active window border.  --> 
		<EGDC_ACTIVE_CAPTION a="255" r="255" g="255" b="0" />		  		<!-- Titulo barra de texto Active window title bar text --> 
		<EGDC_APP_WORKSPACE a="255" r="255" g="255" b="0" />		-->		<!-- NO SE. Background color of multiple document interface (MDI) applications. -->  
		<EGDC_BUTTON_TEXT a="255" r="255" g="255" b="255" />		 		<!-- Color de Texto. --> 
		<EGDC_GRAY_TEXT a="255" r="255" g="255" b="0" />				<!-- Grayed (deshabilitado) text. -->  
		<EGDC_HIGH_LIGHT a="255" r="0" g="0" b="102" />		-->  		<!-- Color del fondo del cuadro cuando pasas sobre los items cuando eliges decks. Item(s) selected in a control. -->  
		<EGDC_HIGH_LIGHT_TEXT a="255" r="255" g="255" b="0" />	  			<!-- Color de los items selecionados o cuando te paras en el. --> 
		<EGDC_INACTIVE_BORDER a="255" r="255" g="255" b="0" />	-->  		<!-- NO SE. Inactive window border. -->  
		<EGDC_INACTIVE_CAPTION a="255" r="255" g="255" b="0" />	        --> 		<!-- Color de texto de ventana Inactiva. Inactive window caption. -->  
		<EGDC_TOOLTIP a="255" r="255" g="255" b="0" />		-->		<!-- NO SE. Tool tip text color. -->  
		<EGDC_TOOLTIP_BACKGROUND a="255" r="255" g="255" b="0" />	-->		<!-- NO SE. Tool tip background color. -->  
		<EGDC_SCROLLBAR a="255" r="20" g="40" b="95" />		-->		<!-- Color de Barra de desplazamiento. Scrollbar gray area. This ONLY applies to the default scrollbar, if you want to change the colors of the graphical one, change the color attributes in the section near the bottom -->  
		<EGDC_WINDOW a="255" r="255" g="255" b="0" />					<!-- tambien es la flechita y la X para cerrar ventantas. Window background. -->  
		<EGDC_WINDOW_SYMBOL a="255" r="255" g="255" b="0" />		  		<!-- flechitas o X. Window symbols like on close buttons, scroll bars and check boxes.  -->
		<EGDC_ICON a="255" r="255" g="255" b="0" />			-->		<!-- NO SE. Icons in a list or tree. -->  
		<EGDC_ICON_HIGH_LIGHT a="255" r="255" g="255" b="0" />	-->		<!-- NO SE. Selected icons in a list or tree. -->
		
		<Caption tbardistancex="2" tbardistancey="9" />
		<WindowButton width="24" />
		<CheckBoxColor a="255" r="255" g="255" b="0" />
	</Global>
		<Custom>
  			<!--Your LP bar-->
			<LPBAR_1_BOTTOM_LEFT a="255" r="255" g="0" b="0" />
			<LPBAR_1_BOTTOM_RIGHT a="255" r="40" g="199" b="29" />
			<LPBAR_1_TOP_LEFT a="255" r="255" g="0" b="0" />
			<LPBAR_1_TOP_RIGHT a="255" r="40" g="199" b="29" />
			<!--Opponent's LP bar-->
			<LPBAR_2_BOTTOM_LEFT a="255" r="40" g="199" b="29" />
			<LPBAR_2_BOTTOM_RIGHT a="255" r="255" g="0" b="0" />
			<LPBAR_2_TOP_LEFT a="255" r="40" g="199" b="29" />
			<LPBAR_2_TOP_RIGHT a="255" r="255" g="0" b="0" />
            <DUELFIELD_TOOLTIP_TEXT_COLOR a="255" r="255" g="255" b="255" />
            <DUELFIELD_TOOLTIP_TEXT_BACKGROUND_COLOR a="255" r="7" g="49" b="125" />
            <DUELFIELD_CARD_SELF_WINDOW_BACKGROUND a="255" r="20" g="140" b="255" />
            <DUELFIELD_CARD_OPPONENT_WINDOW_BACKGROUND a="200" r="180" g="90" b="255" />
            <CARDINFO_IMAGE_BACKGROUND a="255" r="30" g="36" b="102" />
            <CARDINFO_TYPES_COLOR a="255" r="25" g="210" b="255" />
            <CARDINFO_STATS_COLOR a="255" r="80" g="250" b="50" />
            <CARDINFO_ARCHETYPE_TEXT_COLOR a="255" r="255" g="50" b="10" />
            <CARDINFO_PASSCODE_SCOPE_TEXT_COLOR a="255" r="242" g="255" b="0" />
            <DUELFIELD_CARD_SELECT_WINDOW_SET_TEXT a="255" r="255" g="255" b="255" />
			
			
			<!-- Changes the color for the texts in the room list menu. Examples: the "Server", "Nickname", "show locked rooms" texts-->
			<ROOMLIST_TEXTS_COLOR a="255" r="255" g="255" b="255" />
			<!-- Changes the color for a room that didn't have any of its settings changed from the default ones-->
			<ROOMLIST_NORMAL_ROOM a="255" r="0" g="150" b="0" />
			<!-- Changes the color for a room that has any of its settings changed from the default ones. Example: different LP, different starting hand, etc-->
			<ROOMLIST_CUSTOM_ROOM a="255" r="182" g="255" b="0" />
			<!-- Changes the color for a room, in the room list, that has already started its duel.-->
			<ROOMLIST_STARTED_ROOM a="255" r="178" g="0" b="255" />

	</Custom>
  	<Button texture="button.png">
		<SrcBorder tlx="10" tly="10" brx="10" bry="10" />
		<DstBorder tlx="10" tly="10" brx="10" bry="10" />
		<Pressed texture="ButtonPressed.png">
			<SrcBorder tlx="10" tly="10" brx="10" bry="10" />
			<DstBorder tlx="10" tly="10" brx="10" bry="10" />
			<Color r="255" g="255" b="255" a="255" />
		</Pressed>
		<ButtonDisabled texture="ButtonDisabled.png">
			<SrcBorder tlx="10" tly="10" brx="10" bry="10" />
			<DstBorder tlx="10" tly="10" brx="10" bry="10" />
			<Color r="255" g="255" b="255" a="255" />
		</ButtonDisabled>
	</Button>

	<MenuBar texture="ButtonPressed.png">
		<SrcBorder tlx="10" tly="10" brx="10" bry="10" />
		<DstBorder tlx="10" tly="10" brx="10" bry="10" />
		<Color r="255" g="255" b="255" a="150" />
		<MenuPane texture="sunkenPane.png">
			<SrcBorder tlx="3" tly="3" brx="3" bry="3" />
			<DstBorder tlx="3" tly="3" brx="3" bry="3" />
			<Color r="255" g="255" b="255" a="255" />
		</MenuPane>
		<MenuPressed texture="buttonPressed.png">
			<SrcBorder tlx="3" tly="3" brx="3" bry="3" />
			<DstBorder tlx="3" tly="3" brx="3" bry="3" />
			<Color r="255" g="255" b="255" a="255" />
		</MenuPressed>
	</MenuBar>

	<CheckBox texture="CheckBox.png">
		<SrcBorder tlx="1" tly="1" brx="1" bry="1" />
		<DstBorder tlx="1" tly="1" brx="1" bry="1" />
		<CheckBoxDisabled texture="CheckBoxDisabled.png">
			<SrcBorder tlx="1" tly="1" brx="1" bry="1" />
			<DstBorder tlx="1" tly="1" brx="1" bry="1" />
		</CheckBoxDisabled>
	</CheckBox>

	<ComboBox texture="sunkenpane.png">
		<SrcBorder tlx="1" tly="1" brx="1" bry="1" />
		<DstBorder tlx="1" tly="1" brx="1" bry="1" />
		<ComboBoxDisabled texture="CheckBox.png">
			<SrcBorder tlx="1" tly="1" brx="1" bry="1" />
			<DstBorder tlx="1" tly="1" brx="1" bry="1" />
			<Color r="255" g="255" b="255" a="255" />
		</ComboBoxDisabled>
	</ComboBox>

	<SunkenPane texture="sunkenpane.png">
		<SrcBorder tlx="10" tly="10" brx="10" bry="10" />
		<DstBorder tlx="10" tly="10" brx="10" bry="10" />
		<Color a="226" r="255" g="255" b="255" />
	</SunkenPane>

	<TabControl texture="TabControl.png">
		<SrcBorder tlx="10" tly="10" brx="10" bry="10" />
		<DstBorder tlx="10" tly="10" brx="10" bry="10" />
		<TabButton texture="TabButton.png">
			<SrcBorder tlx="10" tly="10" brx="10" bry="10" />
			<DstBorder tlx="10" tly="10" brx="10" bry="10" />
		</TabButton>
		<TabButtonPressed texture="TabButtonPressed.png">
			<SrcBorder tlx="10" tly="10" brx="10" bry="10" />
			<DstBorder tlx="10" tly="10" brx="10" bry="10" />
		</TabButtonPressed>
	</TabControl>
  
	<Window texture="windowbg.png">
		<Color a="255" r="255" g="255" b="255" />
		<SrcBorder tlx="25" tly="10" brx="10" bry="10" />
		<DstBorder tlx="25" tly="10" brx="10" bry="10" />
	</Window>
	
	<ProgressBar texture="gauge1.png">
		<SrcBorder tlx="16" tly="16" brx="16" bry="16" />
		<DstBorder tlx="16" tly="16" brx="16" bry="16" />
		<Color a="150" r="255" g="255" b="255"/>
		<Filled texture="gauge2.png">
			<SrcBorder tlx="16" tly="16" brx="16" bry="16" />
			<DstBorder tlx="16" tly="16" brx="16" bry="16" />
			<Color a="255" r="255" g="255" b="255" />
		</Filled>
  </ProgressBar>
</Skin>
